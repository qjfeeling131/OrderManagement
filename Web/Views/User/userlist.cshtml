@{
    Layout = "~/views/template/content.cshtml";
}


@section title{ 经销商平台 | 下属用户 }


@section content_header{

    <h1>
        下属用户
        <small>列表</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="~/home/home"><i class="glyphicon glyphicon-home"></i>主页</a></li>
        <li class="active">下属用户</li>
    </ol>
}


@section content{

    <div class="box box-primary">
        <div class="box-header">
            <h3 class="box-title">重设密码</h3>
        </div><!-- /.box-header -->
        <div class="box-body">
            <div id="example2_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                <div class="row">
                    <div class="col-sm-6"></div><div class="col-sm-6"></div>
                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <table id="example2" class="table table-bordered table-hover dataTable" role="grid" aria-describedby="example2_info">
                            <thead>
                                <tr role="row">
                                    <th tabindex="0">Rendering engine</th>
                                    <th tabindex="0">Browser</th>
                                    <th tabindex="0">Platform(s)</th>
                                    <th tabindex="0">Engine version</th>
                                    <th tabindex="0">CSS grade</th>
                                    <th tabindex="0">操作</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr role="row">
                                    <td>Gecko</td>
                                    <td>Seamonkey 1.1</td>
                                    <td>Win 98+ / OSX.2+</td>
                                    <td>1.8</td>
                                    <td>A</td>
                                    <td><button class="btn btn-primary">重设密码</button></td>
                                </tr>
                                <tr role="row">
                                    <td>Webkit</td>
                                    <td>Safari 3.0</td>
                                    <td>OSX.4+</td>
                                    <td>522.1</td>
                                    <td>A</td>
                                </tr>
                                <tr role="row">
                                    <td class="">Webkit</td>
                                    <td class="sorting_1">Safari 2.0</td>
                                    <td>OSX.4+</td>
                                    <td>419.3</td>
                                    <td>A</td>
                                </tr>
                                <tr role="row">
                                    <td class="">Webkit</td>
                                    <td class="sorting_1">Safari 1.3</td>
                                    <td>OSX.3</td>
                                    <td>312.8</td>
                                    <td>A</td>
                                </tr>

                            </tbody>

                        </table>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-5"><div class="dataTables_info">Showing 1 to 10 of 57 entries</div></div>
                    <div class="col-sm-7">
                        <div class="dataTables_paginate" id="paginate">
                            <ul class="pagination">
                                <li class="paginate_button  disabled"> <a href="#">Previous</a> </li>
                                <li class="paginate_button active"><a href="#" >1</a></li>
                                <li class="paginate_button "><a href="#" >2</a></li>
                                <li class="paginate_button "><a href="#" >3</a></li>
                                <li class="paginate_button "><a href="#" >4</a></li>
                                <li class="paginate_button "><a href="#" >5</a></li>
                                <li class="paginate_button "><a href="#" >6</a></li>
                                <li class="paginate_button "><a href="#" >Next</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- /.box-body -->
    </div>

}

@section script{
    @*<script src="~/Js/utility/jquery.min.js"></script>*@
    <script>
        $(function () {
            $("#userManager").addClass("active");
            $("#userManager .treeview-menu").attr("display", "block");
            $("#userManager .treeview-menu>li").eq(1).addClass("active");
        })
    </script>
}
